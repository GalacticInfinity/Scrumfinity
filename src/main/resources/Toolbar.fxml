<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<ToolBar prefHeight="40.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng302.group5.controller.mainAppControllers.ToolBarController">
  <items>
    <Button fx:id="saveButton" mnemonicParsing="false" onAction="#btnSaveClick" text="Save" />
      <Button fx:id="saveAsButton" mnemonicParsing="false" onAction="#btnSaveAsClick" text="Save As" />
      <Button fx:id="loadButton" mnemonicParsing="false" onAction="#btnLoadClick" text="Load" />
      <Separator orientation="VERTICAL" />
      <Button fx:id="undoButton" mnemonicParsing="false" onAction="#btnUndoClick" text="Undo" />
      <Button fx:id="redoButton" mnemonicParsing="false" onAction="#btnRedoClick" text="Redo" />
      <Separator orientation="VERTICAL" />
      <Button fx:id="editButton" mnemonicParsing="false" onAction="#btnEditClick" text="Edit" />
      <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#btnDeleteClick" text="Delete" />
      <Separator orientation="VERTICAL" />
      <Button fx:id="dependenciesButton" mnemonicParsing="false" onAction="#btnDependenciesClick" text="Dependencies" />
  </items>
</ToolBar>
